    0% {
        -webkit-transform: translate(-50%,-50%) rotate(0deg);
        transform: translate(-50%,-50%) rotate(0)
    }

    to {
        -webkit-transform: translate(-50%,-50%) rotate(359deg);
        transform: translate(-50%,-50%) rotate(359deg)
    }
}

