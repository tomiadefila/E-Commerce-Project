<!-- Facebook pixel events by Codematic-->
<script>
    window.addEventListener("load", function () {
        /*
        Import into sections/product.liquid

        Relies on AddToCart button defined by snippets/pdp-form.liquid
        and the product object defined in section templates/product.liquid
        */
        function fbAddToCart(variant_id) {
            // wait a few milliseconds for the cart to be updated to get more info on the variant
            setTimeout(function () {
                fetch("/cart.js")
                    .then(resp => {
                        return resp.json()
                    })
                    .then(function (cart) {
                        cart.items.forEach(function (item) {
                            if (item.id === variant_id) {
                                fbq('track', 'AddToCart',
                                    {
                                        content_type: "product_group",
                                        content_name: item.product_title,
                                        num_items: 1,
                                        content_ids: [item.product_id], // integer. Align with BE
                                        value: item.price / 100,
                                        currency: cart.currency,
                                        content_category: item.product_type,
                                        custom: {
                                            variant_id: variant_id,
                                            variant_title: item.title,
                                            sku: item.sku
                                        }
                                    },
                                    {
                                        eventID: `${cart.token}-${item.product_id}`
                                    }
                                );
                            }
                        })
                    })
                    .catch(function (error) {
                        console.log(error);
                    });
            }, 250)
        }

        let addToCartBtn_ = document.getElementById("AddToCart");
        if( addToCartBtn_ )
        addToCartBtn_.addEventListener("click", function (e) {
            let selected_variant_id = parseInt(document.getElementById("VariantId").value)
            fbAddToCart(selected_variant_id);
        })
    });
</script>
<!-- Facebook pixel events by Codematic-->

<script>
    window.addEventListener("load", function () {
        /*
        Import into sections/collection.liquid and templates/search.liquid

        Relies on AddToCart button and a tags defined by pdp-form.liquid snippet.
        */
        function fbAddToCart(variant_id) {
            // wait a few milliseconds for the cart to be updated to get more info on the variant
            setTimeout(function () {
                fetch("/cart.js")
                    .then(resp => {
                        return resp.json()
                    })
                    .then(function (cart) {
                        cart.items.forEach(function (item) {
                            if (item.id === variant_id) {
                                fbq('track', 'AddToCart',
                                    {
                                        content_type: "product_group",
                                        content_name: item.product_title,
                                        num_items: 1,
                                        content_ids: [item.product_id], // integer. Align with BE
                                        value: item.price / 100,
                                        currency: cart.currency,
                                        content_category: item.product_type,
                                        custom: {
                                            variant_id: variant_id,
                                            variant_title: item.title,
                                            sku: item.sku
                                        }
                                    },
                                    {
                                        eventID: `${cart.token}-${item.product_id}`
                                    }
                                );
                            }
                        })
                    })
                    .catch(function (error) {
                        console.log(error);
                    });
            }, 250)
        }

        let quick_add_selector = 'a[data-quick-add-to-cart]'
        let quickAddToCartHrefs_ = document.querySelectorAll(quick_add_selector)

        quickAddToCartHrefs_.forEach(function (quickAddToCartHref_) {
            quickAddToCartHref_.addEventListener("click", function (e) {
                let variant_id = parseInt(e.target.getAttribute("data-quick-add-to-cart"))
                fbAddToCart(variant_id)
            })
        });

    });
</script>
