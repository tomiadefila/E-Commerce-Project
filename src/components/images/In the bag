  const closeSizeGuideBtn = document.getElementsByClassName("pdp-main__size-guide-close")[0];
  closeSizeGuideBtn.addEventListener("click", () => {
    const wrapper = document.getElementsByClassName("collection-size-guide")[0];
    wrapper.classList.remove("visible");
  });

  const tabs = document.querySelectorAll(".pdp-main__size-guide-category button");
  tabs.forEach((tab, index) => {
    tab.addEventListener("click", () => {
      $(".collection-size-guide").attr('data-active-guide', index)
    });
