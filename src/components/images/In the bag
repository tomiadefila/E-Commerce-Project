<script>
function removeOrFromMessage() {
  var squarePlacement = document.querySelector('square-placement');
  if (squarePlacement && squarePlacement.shadowRoot) {
    var shadowRoot = squarePlacement.shadowRoot;

    var element = shadowRoot.querySelector(".afterpay-main-text");

    if (element) {
      var text = element.textContent;
      text = text.replace("or", "");
      element.textContent = text;
    }
  }
}

document.addEventListener("DOMContentLoaded", function() {
  setTimeout(removeOrFromMessage, 1300);
  setTimeout(removeOrFromMessage, 2000);
});
</script>

